<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:background="@drawable/sign19"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"

    tools:context="com.example.random.mapsapplication.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="Gallary"
        android:id="@+id/button2"
        android:background="@drawable/rounded_edittext"
        android:onClick="upload"
        android:layout_x="278dp"
        android:layout_y="214dp"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView"
        android:layout_below="@+id/imageView" />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:background="@drawable/puntitled"

        android:contentDescription=""
        android:layout_x="251dp"
        android:layout_y="93dp"
        android:layout_below="@+id/imageView2"
        android:layout_toRightOf="@+id/imageView2"
        android:layout_toEndOf="@+id/imageView2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="  Full Name"
        android:ems="10"
        android:id="@+id/editText3"
        android:background="@drawable/rounded_edittext"
        android:layout_marginTop="36dp"
        android:layout_x="-37dp"
        android:layout_y="319dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Signup"
        android:id="@+id/button"
        android:background="@drawable/rounded_edittext"
        android:layout_x="133dp"
        android:layout_y="444dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/textView"
        android:layout_alignRight="@+id/button2"
        android:layout_alignEnd="@+id/button2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText5"
        android:background="@drawable/rounded_edittext"
        android:layout_x="39dp"
        android:layout_y="157dp"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/editText4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/editText4"
        android:layout_alignEnd="@+id/editText4" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:background="@drawable/tran"
        android:layout_x="83dp"
        android:layout_y="-5dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="  Username"
        android:ems="10"
        android:id="@+id/editText"
        android:background="@drawable/rounded_edittext"
        android:layout_x="-37dp"
        android:layout_y="319dp"
        android:layout_marginBottom="60dp"
        android:layout_below="@+id/imageView"
        android:layout_toRightOf="@+id/textView"
        android:layout_toEndOf="@+id/textView" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="  Password"
        android:ems="10"
        android:id="@+id/editText2"
        android:background="@drawable/rounded_edittext"
        android:layout_x="-37dp"
        android:layout_y="319dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button2"
        android:layout_alignEnd="@+id/button2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="  E-mail"
        android:ems="10"
        android:id="@+id/editText4"
        android:background="@drawable/rounded_edittext"
        android:layout_x="-37dp"
        android:layout_y="319dp"
        android:layout_below="@+id/editText2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="35dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="Get Address"
        android:id="@+id/button3"
        android:onClick="address"
        android:background="@drawable/rounded_edittext"
        android:layout_marginTop="30dp"
        android:layout_alignTop="@+id/editText5"
        android:layout_alignLeft="@+id/imageView2"
        android:layout_alignStart="@+id/imageView2"
        android:layout_toLeftOf="@+id/button2"
        android:layout_toStartOf="@+id/button2" />


</RelativeLayout>
